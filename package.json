{
  "name": "next-template",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "check": "node scripts/check-project.js",
    "deploy-check": "node scripts/deploy-check.js",
    "validate-config": "node scripts/validate-config.js",
    "test-deployment": "node scripts/test-deployment.js",
    "pre-deploy": "npm run validate-config && npm run deploy-check && npm run test-deployment",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
